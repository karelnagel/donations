{
  "name": "ethdon",
  "license": "UNLICENSED",
  "scripts": {
    "prepare:rinkeby": "mustache networks/rinkeby.json subgraph.template.yaml > subgraph.yaml",
    "codegen": "graph codegen",
    "build": "graph codegen && graph build",
    "deploy:main": "graph deploy --product hosted-service karelnagel/ethdon",
    "deploy:dev": "graph deploy --product hosted-service karelnagel/ethdon-dev",
    "create-local": "graph create --node http://localhost:8020/ ethdon",
    "remove-local": "graph remove --node http://localhost:8020/ ethdon",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 ethdon"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.29.0",
    "@graphprotocol/graph-ts": "0.26.0"
  },
  "devDependencies": {
    "mustache": "^4.2.0"
  }
}